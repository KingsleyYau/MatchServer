# 外部服务配置
[BASE]
# 端口(内部端口为此端口+1,例如9877为内部端口)
PORT=9876
# 最大连接数
MAXCLIENT=100000
# 内存copy数目, 一般为cpu内核数目
MAXMEMORYCOPY=4
# 处理线程数目, 一般为cpu内核数目
MAXHANDLETHREAD=4
# 单核每秒处理请求数目
MAXQUERYPERCOPY=50
# 单个请求超时时间(秒)
TIMEOUT=5
# 统计信息输出时间间隔(秒)
STATETIME=60

# 日志配置
[LOG]
# 日志级别(0-5, 0:关闭, 5:全开)
LOGLEVEL=5
# 日志路径
LOGDIR=./log
# 调试模式(0:关闭, 1:开启)
DEBUGMODE=0

# mysql数据库配置
[DB]
# 问题库ip
DBHOST=127.0.0.1
# 问题库端口
DBPORT=3306
# 问题库用户名
DBUSER=root
# 问题库密码
DBPASS=123456
# 问题库数据名字
DBNAME=qpidnetwork
# 问题库连接池数目
MAXDATABASETHREAD=1
# 问题库同步时间间隔(分钟)
SYNCHRONIZETIME=30
# 在线库同步时间间隔(分钟)
SYNCHRONIZE_ONLINE_TIME=1
# 在线库数目(最多4个)
ONLINE_DB_COUNT=4

# 在线库格式[DB_ONLINE]
# CL
[DB_ONLINE_0]
# 在线库ip
DBHOST=127.0.0.1
# 在线库端口
