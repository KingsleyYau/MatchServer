# 外部服务配置
[BASE]
# 端口(内部端口为此端口+1,例如9877为内部端口)
PORT=9876
# 最大连接数
MAXCLIENT=100000
# 内存copy数目, 一般为cpu内核数目
MAXMEMORYCOPY=2
# 处理线程数目, 一般为cpu内核数目
MAXHANDLETHREAD=2
# 单核每秒处理请求数目
MAXQUERYPERCOPY=20
# 单个请求超时时间(秒)
TIMEOUT=5
# 统计信息输出时间间隔(秒)
STATETIME=60

# 日志配置
[LOG]
# 日志级别(0-5, 0:关闭, 5:全开)
LOGLEVEL=4
# 日志路径
LOGDIR=./log
# 调试模式(0:关闭, 1:开启)
DEBUGMODE=0

# mysql数据库配置
[DB]
# ip
DBHOST=127.0.0.1
# 端口
DBPORT=3306
# 用户名
DBUSER=root
# 密码
DBPASS=123456
# 数据名字
DBNAME=qpidnetwork
# 连接池数目
MAXDATABASETHREAD=4
# 同步时间间隔(秒)
SYNCHRONIZETIME=30
