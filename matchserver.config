# 在线库用户名
DBUSER=root
# 在线库密码
DBPASS=123456
# 在线库数据名字
DBNAME=qpidnetwork_online_cl
# 在线库连接池数目
MAXDATABASETHREAD=1
# 站点Id
SITEID=0

